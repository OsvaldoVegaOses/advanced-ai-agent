{
  "routes": [
    {
      "route": "/api/health",
      "rewrite": "https://advanced-ai-agent-0003.azurewebsites.net/health",
      "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD", "PATCH"]
    },
    {
      "route": "/api/chat",
      "rewrite": "https://advanced-ai-agent-0003.azurewebsites.net/chat",
      "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD", "PATCH"]
    },
    {
      "route": "/api/conversations",
      "rewrite": "https://advanced-ai-agent-0003.azurewebsites.net/conversations",
      "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD", "PATCH"]
    },
    {
      "route": "/api/*",
      "rewrite": "https://advanced-ai-agent-0003.azurewebsites.net/*",
      "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD", "PATCH"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/_next/*", "/static/*", "/*.{js,css,png,jpg,gif,ico,svg}"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY", 
    "X-XSS-Protection": "1; mode=block"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".html": "text/html"
  }
}